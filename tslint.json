{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "no-empty-interface": false,
    "quotemark": false,
    "only-arrow-functions": false,
    "ban-types": false,
    "object-literal-sort-keys": false,
    "variable-name": [true, "allow-leading-underscore"],
    "no-console": false
  },
  "rulesDirectory": []
}
